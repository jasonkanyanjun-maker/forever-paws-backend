{
  "name": "forever-paws-backend",
  "version": "1.0.0",
  "description": "Forever Paws Pet Memorial App Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/start.ts",
    "build": "tsc",
    "build:render": "tsc --project tsconfig.render.json",
    "start": "node dist/start.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "migrate": "npm run build && node scripts/migrate-database.js migrate",
    "health-check": "node scripts/health-check.js check",
    "deploy-check": "node scripts/pre-deploy-check.js",
    "production-check": "node scripts/production-deploy-check.js",
    "migration-check": "node scripts/database-migration-check.js",
    "validate-deployment": "node scripts/deployment-validation.js",
    "setup-monitoring": "node scripts/monitoring-setup.js",
    "security-audit": "node scripts/security-audit.js",
    "deployment-checklist": "node scripts/deployment-checklist.js",
    "complete-deployment-check": "node scripts/complete-deployment-check.js",
    "performance-optimization": "node scripts/performance-optimization.js",
    "fix-security": "node scripts/fix-security-issues.js",
    "db:migrate": "node scripts/migrate-database.js migrate",
    "db:seed": "node scripts/migrate-database.js seed",
    "db:create": "node scripts/migrate-database.js create",
    "db:status": "node scripts/migrate-database.js status",
    "monitor": "node scripts/health-check.js monitor",
    "security-check": "npm audit && npm run security-audit"
  },
  "keywords": [
    "pet",
    "memorial",
    "api",
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "Forever Paws Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.18.0 <21.0.0",
    "npm": ">=8.0.0"
  }
}