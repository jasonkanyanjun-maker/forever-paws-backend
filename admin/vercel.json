{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://forever-paws-api-production.up.railway.app",
    "VITE_APP_TITLE": "Forever Paws 后台管理系统",
    "VITE_APP_VERSION": "1.0.0"
  }
}